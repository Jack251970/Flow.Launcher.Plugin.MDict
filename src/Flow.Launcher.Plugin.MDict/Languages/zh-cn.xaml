<?xml version="1.0" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--  General  -->
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_name">MDict</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_description">在 MDict 词典中搜索词条</system:String>

    <!--  Main  -->
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_please_select_dictionary">请在设置中选择一个词典文件</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_please_type_query">请输入内容以进行查询</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_error_loading_dictionary">无法加载该词典！请重新选择一个词典文件</system:String>

    <!--  Settings  -->
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_dictionary_path">词典文件路径：</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_select">选择</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_md_files">MDict 文件</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_light_theme_css">浅色主题 CSS 相对路径：</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_dark_theme_css">深色主题 CSS 相对路径：</system:String>
    <system:String x:Key="flowlauncher_plugin_mdict_plugin_enable_spell_check">对搜索输入启用拼写检查</system:String>

</ResourceDictionary>